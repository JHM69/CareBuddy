<resources>    <!-- Base application theme. -->    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">        <!-- Customize your theme here. -->        <item name="colorPrimary">@color/colorPrimaryDark</item>        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>        <item name="colorAccent">@color/colorAccent</item>        <item name="windowActionBar">false</item>        <item name="windowNoTitle">true</item>    </style>    <style name="ToolbarTheme" parent="@style/ThemeOverlay.AppCompat.ActionBar">        <!-- Customize color of navigation drawer icon and back arrow -->        <item name="colorControlNormal">@color/textColorPrimary</item>    </style></resources>