<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.beffjezos.carebuddy">    <uses-permission android:name="android.permission.CALL_PHONE" />    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.INTERNET" />    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity android:name="com.beffjezos.carebuddy.ui.activities.CharityActivity"></activity>        <activity android:name="com.beffjezos.carebuddy.ui.activities.DonatePackage" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.VerifyActivity" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.DonateMoney" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.CategoryActivity" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.CharitiesList" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.NewCharityReg" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.NewDonorReg" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.MainActivity" />        <activity android:name="com.beffjezos.carebuddy.ui.activities.Splash">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity            android:name="com.yalantis.ucrop.UCropActivity"            android:screen